Matrix Array :

import java.util.*;
public class MatrixArray{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        int m = scan.nextInt();
        int[][] arr = new int[n][m];
        for(int i=0;i<n;i++){
            for(int j=0;j<m;j++){
                arr[i][j] = scan.nextInt();
            }
        }
        for(int i=0;i<n;i++){
            int max = 0;
            for(int j=0;j<m;j++){
                if(arr[i][j]>max){
                    max = arr[i][j];
                }
                
            }
        System.out.print(max+" ");
        }

    }
}
